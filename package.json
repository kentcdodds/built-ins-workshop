{
  "name": "built-ins-workshop",
  "description": "The practical bits of JavaScript's \"standard library\" that will make you more productive with JavaScript",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/kentcdodds/built-ins-workshop",
  "repository": {
    "type": "git",
    "url": "git://github.com/kentcdodds/built-ins-workshop.git"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "common-tags": "^1.4.0",
    "eslint": "^3.16.1",
    "eslint-config-kentcdodds": "^12.0.0",
    "eslint-plugin-babel": "^4.1.0",
    "glob": "^7.1.1",
    "husky": "^0.13.1",
    "jest-cli": "^19.0.2",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "opt-cli": "^1.5.1",
    "prettier-eslint-cli": "^3.1.1",
    "pretty-format": "^19.0.0",
    "replace-in-file": "^2.4.0",
    "split-guide": "^1.2.1"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate",
    "start": "npm run test:watch",
    "lint": "eslint exercises-final common quizzes",
    "format:quizzes": "prettier-eslint --write \"quizzes/**/*.js\"",
    "format:templates": "prettier-eslint --write \"templates/**/*.js\"",
    "format:exercises": "prettier-eslint --write \"exercises*/**/*.js\"",
    "precommit": "opt --in precommit --exec \"npm-run-all precommit:*\"",
    "precommit:1": "npm-run-all --parallel format:templates format:quizzes lint generate",
    "precommit:2": "npm-run-all --parallel test:final format:exercises",
    "precommit:3": "git add exercises exercises-final",
    "quiz": "babel-node quizzes/run.js",
    "test": "jest --config=exercises/jest.config.json",
    "test:changed": "npm run test --silent -- --onlyChanged",
    "test:watch": "npm test -- --watch",
    "test:final": "jest --config=exercises-final/jest.config.json",
    "test:final:watch": "npm run test:final -- --watch",
    "dev": "npm run generate && npm-run-all --parallel generate:watch test:final:watch",
    "generate:watch": "onchange \"templates/**/*.*\" --initial -- npm run generate",
    "generate": "split-guide generate --silent-success",
    "autofill-email": "node ./scripts/autofill-feedback-email",
    "validate": "npm run lint && npm run test:final",
    "setup": "node ./scripts/verify && node ./scripts/install && npm run validate"
  },
  "LICENSE": "MIT"
}
